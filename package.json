{
  "name": "bdsolapi",
  "version": "0.0.8",
  "description": "La BDSOL disponible grâce à docker, avec toutes ses facettes. Principalement pour le developpement.",
  "main": "src/api.ts",
  "private": true,
  "scripts": {
    "tsc": "tsc && tsc-alias",
    "serve": "node build/src/server.js",
    "serve-ts": "npx ts-node src/server.ts",
    "backup-ts": "npx ts-node -r tsconfig-paths/register src/backup.ts",
    "restore-ts": "npx ts-node -r tsconfig-paths/register src/restore.ts",
    "dev": "npx nodemon -r tsconfig-paths/register src/server.ts",
    "dev-old": "npx ts-node-dev --respawn --transpile-only --debug --watch src src/server.ts",
    "prod": "tsc && node ./build/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-helpers": "npx ts-node ./src/helpers.ts",
    "lint": "eslint . --ext .ts",
    "generate-keys": "ssh-keygen -t rsa -b 4096 -C bdsol-api -f bdsolapi"
  },
  "keywords": [],
  "authors": [
    "Marc-André Martin",
    "Frédéric Rivard"
  ],
  "license": "à venir",
  "dependencies": {
    "@braintree/sanitize-url": "^6.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/http-server": "^0.12.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash.deburr": "^4.1.7",
    "@types/mime-db": "^1.43.1",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.2.5",
    "@types/node-schedule": "^2.1.0",
    "@types/nodemailer": "^6.4.11",
    "@types/nunjucks": "^3.2.4",
    "@types/sanitize-html": "^2.6.2",
    "argon2": "^0.28.7",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^7.*",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "lodash.deburr": "^4.1.0",
    "mime-db": "^1.52.0",
    "mime-types": "^2.1.35",
    "mongodb": "^6",
    "mongoose": "^7.5",
    "mongoose-slug-updater": "^3.3.0",
    "multer": "^1.4.5-lts.1",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.6",
    "nunjucks": "^3.2.4",
    "sanitize-html": "^2.7.1",
    "ts-migrate-mongoose": "^3.1.6",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/express-list-endpoints": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "express-list-endpoints": "^6.0.0",
    "nodemon": "^3"
  }
}
