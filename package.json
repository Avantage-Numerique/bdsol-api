    "@types/jsonwebtoken": "^8.5.8",
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.3",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.4.1",
